{
  "name": "lets-move",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "csslint": "^1.0.5",
    "howler": "^2.0.2",
    "jscs": "^3.0.7",
    "now": "^4.1.2",
    "react-scripts": "^0.8.5",
    "uuid": "^3.0.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "jscs src",
    "deploy": "npm build && rm build/sound/*.wav && now build",
    "mp3s": "ls public/sound/*.wav | xargs -I {} ffmpeg -i {} -acodec libmp3lame -ab 128k {}.mp3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "browser": true,
    "notify": true,
    "testEnvironment": "jsdom"
  }
}
